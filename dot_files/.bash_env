export PRY_WANTED=true
export OCULUS_ENABLED=1
export LIVERELOAD=1

export STYLE_ENFORCEMENT=1
export AUTO_MIGRATE=1
export GUARD_BUNDLE=1

#export SSL_CERT_DIR=/etc/pki/tls/certs
export SSL_CERT_FILE=/etc/pki/tls/cert.pem

export rvmsudo_secure_path=1


export JAVA_HOME=/usr/lib/jvm/jre
export PATH=$PATH:$JAVA_HOME
export JRE_HOME=$JAVA_HOME/jre


export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$JAVA_HOME #/jre/lib/amd64:$JAVA_HOME/jre/lib/amd64/client

export PATH="$HOME/.linuxbrew/bin:$PATH"
export MANPATH="$HOME/.linuxbrew/share/man:$MANPATH"
export INFOPATH="$HOME/.linuxbrew/share/info:$INFOPATH"
